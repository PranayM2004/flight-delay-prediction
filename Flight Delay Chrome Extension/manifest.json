{
    "manifest_version": 3,
    "name": "Flight Information Extension",
    "version": "1.0",
    "description": "An extension to check flight information",
    "icons": {
      "128": "icon.png",
      "48": "icon.png",
      "16": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": ["activeTab", "storage"]
  }
  